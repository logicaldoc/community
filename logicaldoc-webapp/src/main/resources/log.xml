      <Policies>
        <SizeBasedTriggeringPolicy size="5 MB" />
      </Policies>
      <DefaultRolloverStrategy max="5" />
    </RollingFile>
    <RollingFile name="STORAGE_DELETIONS" fileName="${root}/storage_deletions.log" filePattern="${root}/storage_deletions.log.%i">
      <PatternLayout>
        <Pattern>%d{dd.MM.yyyy HH:mm:ss,SSS} - %m%n</Pattern>
      </PatternLayout>
      <Policies>
        <SizeBasedTriggeringPolicy size="100 MB" />
      </Policies>
      <DefaultRolloverStrategy max="5" />
    </RollingFile>
    <RollingFile name="console_file" fileName="${root}/console.log" filePattern="${root}/console.log.%i">
      <PatternLayout>
        <Pattern>${pattern}</Pattern>
      </PatternLayout>
      <Policies>
        <SizeBasedTriggeringPolicy size="5 MB" />
      </Policies>
      <DefaultRolloverStrategy max="5" />
    </RollingFile>
    <RollingFile name="AI_OPENNLP" immediateFlush="true" fileName="${root}/ai_nlp.log" filePattern="${root}/ai_nlp.log.%i">
      <PatternLayout>
        <Pattern>%d{dd.MM.yyyy HH:mm:ss,SSS} %-5p %m%n</Pattern>
      </PatternLayout>
      <Policies>
        <SizeBasedTriggeringPolicy size="5 MB" />
      </Policies>
      <DefaultRolloverStrategy max="5" />
    </RollingFile>
    <RollingFile name="AI_ORGDEEPLEARNING4J" immediateFlush="true" fileName="${root}/ai_deep.log" filePattern="${root}/ai_deep.log.%i">
      <PatternLayout>
        <Pattern>%d{dd.MM.yyyy HH:mm:ss,SSS} %-5p %m%n</Pattern>
      </PatternLayout>
      <Policies>
        <SizeBasedTriggeringPolicy size="5 MB" />
      </Policies>
      <DefaultRolloverStrategy max="5" />
    </RollingFile>
    <Console name="console" target="SYSTEM_OUT">
      <PatternLayout pattern="%m%n" />
    </Console>
